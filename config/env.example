NODE_ENV=development
PORT=9000
HOST=0.0.0.0
SQLITE_FILENAME=rocket.db
AUTH_PROVIDER=mock
USE_FIREBASE_EMULATOR=false
JWT_SECRET=dev-secret-change-me
SESSION_COOKIE_NAME=sandrocket_session
SESSION_COOKIE_SECURE=false
CORS_ALLOWLIST=http://localhost:5173,http://localhost:9000
CLIENT_ORIGIN=http://localhost:5173

# Firebase client configuration (required when AUTH_PROVIDER is firebase or firebase-emulator)
FIREBASE_API_KEY=
FIREBASE_AUTH_DOMAIN=
FIREBASE_PROJECT_ID=
FIREBASE_STORAGE_BUCKET=
FIREBASE_MESSAGING_SENDER_ID=
FIREBASE_APP_ID=
FIREBASE_MEASUREMENT_ID=

# Firebase admin service account (required when AUTH_PROVIDER=firebase)
FIREBASE_ADMIN_CREDENTIALS=./secure/firebase-admin.json

